{
   "manifest_version": 2,
   "name": "Read on reMarkable",
   "version": "1.2.0",   

   "description": "Read web articles on your reMarkable with a click.",

   "options_page": "options.html",
   "key": "",

   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "256": "icons/256.png",
      "48": "icons/48.png"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",

   "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Read the current page on your reMarkable account."
   },

   "background": {
      "page": "background.html",
      "persistent": false
   },

   "permissions": [
      "activeTab",
      "contextMenus",
      "printerProvider",
      "storage",
      "https://webapp-production-dot-remarkable-production.appspot.com/",
      "https://internal.cloud.remarkable.com/"
   ]
}
